{
  "name": "vue-ondemand-routing",
  "version": "1.0.0",
  "description": "a webpack plugin for vue ondemand routing",
  "keywords": [
    "vue-route",
    "ondemand",
    "typescript",
    "vue2",
    "webpack4",
    "webpack plugin"
  ],
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./definitions": "./dist/definitions.js",
    "./plugin": {
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.js"
    },
    "./hooks": "./dist/hooks.mjs",
    "./emitter": "./dist/emitter.mjs"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "test": "jest --passWithNoTests",
    "lint": "eslint \"**/*.{ts,js}\" --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "build": "pnpm clean && tsup",
    "release": "node ./scripts/release.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node ./scripts/verify-commit-msg.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "author": "Thinker-ljn",
  "license": "MIT",
  "devDependencies": {
    "@nexhome/yorkie": "^2.0.8",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/prettier": "^2.4.0",
    "@types/webpack": "^4",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "axios": "^0.24.0",
    "chalk": "^4.1.2",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "execa": "^5.1.1",
    "jest": "^27.2.1",
    "lint-staged": "^11.1.2",
    "minimist": "^1.2.5",
    "prettier": "^2.4.1",
    "semver": "^7.3.5",
    "ts-jest": "^27.0.5",
    "tsup": "^6.1.0",
    "typescript": "^4.7.3",
    "vue-router": "^3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thinker-ljn/vue-ondemand-routing.git"
  },
  "homepage": "https://github.com/Thinker-ljn/vue-ondemand-routing#readme",
  "dependencies": {
    "fast-glob": "^3.2.11",
    "vue-route-generator": "^1.0.0",
    "webpack-virtual-modules": "^0.4.3"
  },
  "peerDependencies": {
    "vue-router": "^3",
    "webpack": "^4"
  }
}
